<mvc:View
    xmlns:cards="sap.f.cards" xmlns:form="sap.ui.layout.form"
    controllerName="routing.controller.FormView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="" class="custom" showHeader="false">
                    <Bar design="Header" class="h">
                        <contentMiddle>
                            <Label text="Login Form"></Label>
                        </contentMiddle>
                    </Bar>

					<content>
                    	<VBox class="sapUiSmallMarginTopBottom sapUiLargeMarginBeginEnd">
                        <HBox justifyContent="SpaceBetween">
                        <VBox>
                        <Text text="Welcome to eBlotter" class="t"></Text>
                        <Text text="OPSD4 Disbursement Order Management" class="t"></Text>
                        <Image src="data:image/jpeg;base64,"/>
                        </VBox>
                        <VBox>
                        <form:SimpleForm editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4"
                        labelSpanM="4" labelSpanS="12" columnsL="1" columnsM="1" emptySpanL="4" emptySpanM="4">
                            <form:content>
                                <Label text="Username" required="true" class="t"></Label>
                                <Input id="inp_usernameId" ></Input>
                                <Label text="Password" required="true" class="t"></Label>
                                <Input id="inp_passwordId" type="Password" value=""></Input>
                            </form:content>
                        </form:SimpleForm>
                        <Bar design="SubHeader" class="custom">
                            <contentMiddle>
                                <Button text="Login" press="onLoginUser" type="Accept"></Button>
                                <Button text="Cancel" press="onCancelUser" type="Reject"></Button>
                            </contentMiddle>
                        </Bar>
                        </VBox>
                        </HBox>
                        </VBox>
                    </content>
                    
                        
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
